{
  "name": "@ericpinxteren/ts-eventsourcing",
  "private": false,
  "version": "1.0.0",
  "description": "Typed event sourcing framework",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/encore production --config-name='web'",
    "watch": "./node_modules/.bin/encore dev --watch",
    "test": "./node_modules/.bin/jest",
    "update-test": "./node_modules/.bin/jest -u",
    "covarge": "./node_modules/.bin/jest --coverage",
    "tslint-fix": "./node_modules/.bin/tslint --fix --project tsconfig.json",
    "tslint": "./node_modules/.bin/tslint --project tsconfig.json",
    "jscpd": "./node_modules/.bin/jscpd",
    "tsc-check": "./node_modules/.bin/tsc --noEmit --project server.tsconfig.json && ./node_modules/.bin/tsc --noEmit --project web.tsconfig.json "
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/epinxteren/ts-eventsourcing.git"
  },
  "keywords": [
    "typescript",
    "ts",
    "eventsourcing",
    "cqrs",
    "redux"
  ],
  "author": "Eric Pinxteren",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/epinxteren/ts-eventsourcing/issues"
  },
  "homepage": "https://gitlab.com/epinxteren/ts-eventsourcing#README",
  "devDependencies": {
    "@symfony/webpack-encore": "^0.17.1",
    "@types/jest": "^22.1.0",
    "@types/mockdate": "^2.0.0",
    "@types/node": "^9.3.0",
    "@types/uuid-validate": "^0.0.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "ignore-loader": "^0.1.2",
    "jest": "^23.5.0",
    "jscpd": "^0.6.17",
    "mockdate": "^2.0.2",
    "moment": "^2.22.2",
    "nodemon": "^1.14.1",
    "nodemon-webpack-plugin": "^0.1.6",
    "rxjs-marbles": "^2.3.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^3.3.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-immutable": "^4.5.1",
    "tslint-loader": "^3.5.3",
    "tslint-microsoft-contrib": "^5.0.2",
    "typescript": "^3.0.1",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0",
    "webpack-notifier": "^1.5.0"
  },
  "dependencies": {
    "@types/uuid": "^3.4.3",
    "jest-date-mock": "^1.0.3",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "uuid": "^3.2.1",
    "uuid-validate": "^0.0.2"
  }
}
