{
  "name": "ts-eventsourcing",
  "private": false,
  "version": "1.2.1",
  "description": "Typed event sourcing framework",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "publish-build": "rm -fr build && yarn copy-config-files && cd build && yarn tsc && yarn publish && cd -",
    "copy-config-files": "yarn cpy '(package.json|README.md|.npmignore|assets)' build/. && cp tsconfig.build.json build/tsconfig.json && cp -r src build/.",
    "watch": "tsc --watch",
    "test": "npm-run-all --parallel jest-coverage tslint jscpd tsc",
    "jest": "jest",
    "tsc": "tsc",
    "stryker": "stryker run",
    "jest-coverage": "jest --coverage",
    "tslint-fix": "tslint --fix --project tsconfig.json",
    "tslint": "tslint --project tsconfig.json",
    "jscpd": "jscpd --limit=-1",
    "jest-badges": "yarn jest-coverage && jest-coverage-badges"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/epinxteren/ts-eventsourcing.git"
  },
  "keywords": [
    "typescript",
    "ts",
    "eventsourcing",
    "cqrs",
    "commandbus",
    "eventbus"
  ],
  "author": "Eric Pinxteren",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/epinxteren/ts-eventsourcing/issues"
  },
  "homepage": "https://gitlab.com/epinxteren/ts-eventsourcing#README",
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.9.0",
    "@types/mockdate": "^2.0.0",
    "@types/node": "^13.1.8",
    "@types/uuid-validate": "^0.0.1",
    "cpy-cli": "^3.0.0",
    "fs-extra": "^8.1.0",
    "ignore-loader": "^0.1.2",
    "jest": "^24.9.0",
    "jest-coverage-badges": "^1.1.2",
    "jscpd": "^2.0.16",
    "mockdate": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "rxjs-marbles": "^5.0.4",
    "rxjs-tslint": "^0.1.7",
    "stryker": "^0.35.1",
    "stryker-api": "^0.24.1",
    "stryker-html-reporter": "^0.18.1",
    "stryker-jest-runner": "^1.4.1",
    "stryker-typescript": "^0.18.1",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-loader": "^3.5.4",
    "tslint-microsoft-contrib": "^6.2.0",
    "typeorm": "^0.2.22",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@types/extsprintf": "^1.4.1",
    "@types/uuid": "^3.4.6",
    "error-stack-parser": "^2.0.6",
    "immutable": "^4.0.0-rc.12",
    "moment": "^2.24.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "transit-immutable-js": "^0.8.0",
    "transit-js": "^0.8.861",
    "triviality": "^0.8.1",
    "triviality-logger": "^0.1.4",
    "tslib": "^1.10.0",
    "uuid": "^3.4.0",
    "uuid-validate": "^0.0.3"
  }
}
